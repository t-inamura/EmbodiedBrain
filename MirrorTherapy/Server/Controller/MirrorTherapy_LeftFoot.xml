<?xml version="1.0" encoding="utf8" ?>
<world name="linkage">
  <gravity x="0.0" y="-9.8" z="0.0"/> 
  <!-- Set an avator to grasp Obon with left hand imitating right hand. -->
  <instanciate class="Man-nii.xml">
        <set-attr-value name="name" value="Linkage-Robot"/>
        <set-attr-value name="dynamics" value="false"/>
        <set-attr-value name="x" value="100.0"/>
        <set-attr-value name="y" value="45.0"/>
        <set-attr-value name="z" value="40.0"/>
        <set-attr-value name="collision" value="true"/>
        <set-attr-value name="language" value="c++"/>
        <set-attr-value name="implementation" value="./Linkage_LeftFootGrasp.so"/>
        <camera id="1" link="HEAD_LINK" direction="0 -1 1" aspectRatio="1.5" fov="45" position="0.0 0.0 5.0"/>  
  </instanciate>
  <!-- Set a chair to be sit on-->
  <instanciate class="B2_chair.xml">
        <set-attr-value name="name" value="Chair"/>
        <set-attr-value name="dynamics" value="false"/>
        <set-attr-value name="x" value="100.0"/>
        <set-attr-value name="y" value="50.0"/>
        <set-attr-value name="z" value="40.0"/>
        <set-attr-value name="qw" value="0.0"/>  
        <set-attr-value name="qx" value="0.0"/>  
        <set-attr-value name="qy" value="1.0"/>  
        <set-attr-value name="qz" value="0.0"/>  
        <set-attr-value name="scalex" value="0.8"/>  
        <set-attr-value name="scaley" value="0.8"/>  
        <set-attr-value name="scalez" value="0.8"/>  
  </instanciate>
  <!--  Set a wagon to set Obon on -->
  <instanciate class="seLowtable_A.xml">
        <set-attr-value name="name" value="Wagon"/>
        <set-attr-value name="dynamics" value="false"/>
        <set-attr-value name="x" value="100.0"/>
        <set-attr-value name="y" value="40.0"/>
        <set-attr-value name="z" value="85.0"/>
        <set-attr-value name="scalex" value="1.0"/>  
        <set-attr-value name="scaley" value="2.0"/>  
        <set-attr-value name="scalez" value="1.2"/>  
  </instanciate>
  <!-- Set Obont to be grasped.-->
  <instanciate class="seTray.xml">
        <set-attr-value name="name" value="Tray"/>
        <set-attr-value name="dynamics" value="false"/>
        <set-attr-value name="x" value="100.0"/>
        <set-attr-value name="y" value="63.0"/>
        <set-attr-value name="z" value="70.0"/>
        <set-attr-value name="language" value="c++"/>
        <set-attr-value name="implementation" value="./Linkage_Object.so"/>
        <set-attr-value name="collision" value="true"/>
        <set-attr-value name="graspable" value="false"/>  
        <set-attr-value name="graspRadius" value="12.0"/>
        <set-attr-value name="scalex" value="0.5"/>  
        <set-attr-value name="scaley" value="1.0"/>  
        <set-attr-value name="scalez" value="1.0"/>    
  </instanciate>

  <!-- Set house, situation is in the house-->
  <instanciate class="A1_room.xml">
